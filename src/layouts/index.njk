{% extends "base.njk" %}

{% block content %}
  {% for project in projects %}
    <a href="{{ project.path }}">{{ project.title }}</a>
  {% endfor %}
  <div id="landing-video">
    <video autoplay muted loop>
      <source src="videos/twitch_plays_2.webm" type="video/webm">
      <source src="videos/twitch_plays_2.mp4" type="video/mp4">
    </video>
  </div>
{% endblock content %}

{% block extraJS %}
  {% for js in webpack.landing %}
    <script src="/{{ js }}"></script>
  {% endfor %}
{% endblock extraJS %}
